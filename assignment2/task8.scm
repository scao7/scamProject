(define (node value left right)
	(define (display) (print value))
	this
)
(define (newBST value)
	(node value nil nil)
)
(define (displayBST root)
	(define (iter root indent)
		(if (valid? root)
			(begin
			(iter (root 'right) (string+ indent " "))
			(print indent)
			((root 'display))
			(println)
			(iter (root 'left) (string+ indent " "))
			)
		)
	)
	(iter root "")
)
